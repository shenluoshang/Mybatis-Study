<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.whz.mybatis.mapper.DeptMapper">

    <select id="selectEmpAndDeptByStepTwo" resultType="com.whz.mybatis.pojo.Dept"
            parameterType="java.lang.Integer">
        select *
        from t_dept
        where did = #{did};
    </select>
</mapper>
